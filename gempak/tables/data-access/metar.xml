<?xml version="1.0" encoding="UTF-8"?>
<tns:GempakDataAccess xmlns:tns="http://www.example.org/GempakDataAccess" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/GempakDataAccess GempakDataAccess.xsd ">

    <filelabel text="AWIPS DB CONFIGURATION FILE" version="1" />
    <filetype type="MFSF" source="MFMETR" rawtext="MFTEXT" />
    <dbtable name="obs" />
    <rows pyfile="TimeRetriever" pymeth="gettimes" dbkey="refHour">
	<row name="DATE" />
	<row name="TIME" />
    </rows>
    <columns pyfile="StationRetriever" pymeth="getstations" dbkey="-">
	<column name="STID" />
	<column name="STNM" />
	<column name="SLAT" />
	<column name="SLON" />
	<column name="SELV" />
	<column name="STAT" />
	<column name="COUN" />
	<column name="STD2" />
	<column name="SPRI" />
    </columns>
    <parts>
	<part name="SFDT" type="MDREAL" pyfile="StationDataRetriever" pymethdata="getstationdata" pymethhdr="getheader">
	    <parameters>
		<parameter name="PMSL" scale="0" offset="0" bits="0" key="seaLevelPress" />
		<parameter name="TMPC" scale="0" offset="0" bits="0" key="temperature" />
		<parameter name="DWPC" scale="0" offset="0" bits="0" key="dewpoint" />
		<parameter name="SKNT" scale="0" offset="0" bits="0" key="windSpeed" />
		<parameter name="DRCT" scale="0" offset="0" bits="0" key="windDir" />
	    </parameters>
	</part>
	<part name="SFTX" type="MDCHAR" pyfile="StationDataRetriever" pymethdata="getstationtext" pymethhdr="getheader">
	    <parameters>
		<parameter name="TEXT" scale="0" offset="0" bits="0" key="rawMETAR" />
	    </parameters>
	</part>
    </parts>

</tns:GempakDataAccess>
